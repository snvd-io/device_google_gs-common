package {
    default_applicable_licenses: ["Android-Apache-2.0"],
}

cc_binary {
    name: "dump_perf",
    srcs: ["dump_perf.cpp"],
    init_rc: [
        "init.pixel-mm-gs.rc",
        "init.pixel-perf.rc",
    ],
    cflags: [
        "-Wall",
        "-Wextra",
        "-Werror",
    ],
    shared_libs: [
        "libdump",
    ],
    vendor: true,
    relative_install_path: "dump",
}
